{
  "parser": "babel-eslint",
  "extends": ["@sampotts/eslint-config/es6"],
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Plyr": false,
    "jQuery": false,
    "YT": false,
    "Vimeo": false
  },
  "rules": {
    "import/no-cycle": "warn"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
